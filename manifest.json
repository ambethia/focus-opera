{
  "manifest_version": 2,
  "name": "Focus",
  "description": "Unofficial Focus extension for Opera",
  "version": "1.0",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "background": {
    "scripts": ["index.js"]
  }
}
